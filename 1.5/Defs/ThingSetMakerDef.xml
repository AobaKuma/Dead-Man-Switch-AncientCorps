<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingSetMakerDef>
    <defName>DMSAC_MechDropLoot</defName>
    <root Class="ThingSetMaker_MarketValue">
      <fixedParams>
        <filter>
          <thingDefs>
            <li>Tungsteel</li>
            <li>ComponentStamped</li>
            <li>Plasfiber</li>
            <li>DMS_CeramicsPlating</li>
            <li>DMS_Ceramics</li>
            <li>DMS_CombatRation</li>
            <li>DMS_NutrientPort</li>
            <li>DMS_ProstheticLeg</li>
            <li>DMS_ProstheticArm</li>
            <li>DMS_ProstheticEye</li>
            <li>DMS_ProstheticKidney</li>
            <li>DMS_ProstheticLung</li>
            <li>DMS_ProstheticSpine</li>
          </thingDefs>
        </filter>
        <totalMarketValueRange>200~1000</totalMarketValueRange>
      </fixedParams>
    </root>
  </ThingSetMakerDef>
  <ThingSetMakerDef>
    <defName>DMSAC_LootMaker</defName>
    <root Class="ThingSetMaker_MarketValue">
      <fixedParams>
        <filter>
          <thingDefs>
            <li>Tungsteel</li>
            <li>ComponentStamped</li>
            <li>Plasfiber</li>
            <li>DMS_CeramicsPlating</li>
            <li>DMS_Ceramics</li>
            <li>DMS_CombatRation</li>
            <li>DMS_NutrientPort</li>
            <li>DMS_ProstheticLeg</li>
            <li>DMS_ProstheticArm</li>
            <li>DMS_ProstheticEye</li>
            <li>DMS_ProstheticKidney</li>
            <li>DMS_ProstheticLung</li>
            <li>DMS_ProstheticSpine</li>
          </thingDefs>
        </filter>
        <totalMarketValueRange>300~3000</totalMarketValueRange>
      </fixedParams>
    </root>
  </ThingSetMakerDef>

  <ThingSetMakerDef>
    <defName>DMSAC_CargoLoots</defName>
    <root Class="ThingSetMaker_RandomOption">
      <options>
        <li MayRequire="aoba.deadmanswitch.mobiledragoon">
          <weight>1</weight>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <categories>
                  <li>DMS_Module</li>
                </categories>
              </filter>
              <countRange>1~2</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>
        <li>
          <weight>1</weight>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>ComponentControlChip</li>
                  <li>ComponentFireControl</li>
                </thingDefs>
              </filter>
              <countRange>1~2</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>
        <li>
          <weight>3</weight>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>DMS_Gsh30Gatlinggun</li>
                  <li>Shell_AntigrainWarhead</li>
                  <li>ComponentTransmitter</li>
                </thingDefs>
              </filter>
              <countRange>1~1</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>
        <li>
          <weight>3</weight>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>DMS_ZlobinRifle</li>
                  <li>DMS_Apparel_PowerSkelton</li>
                  <li>DMS_Apparel_HazardSuit</li>
                </thingDefs>
              </filter>
              <countRange>2~4</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>
        <li>
          <weight>10</weight>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>ComponentStamped</li>
                  <li>ComponentIndustrial</li>
                  <li>ComponentSpacer</li>
                </thingDefs>
              </filter>
              <countRange>2~10</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>
        <li>
          <weight>10</weight>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>Shell_HighExplosive</li>
                  <li>Shell_EMP</li>
                </thingDefs>
              </filter>
              <countRange>5~20</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>
        <li>
          <weight>50</weight>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>DMS_EncryptedTransmitter</li>
                </thingDefs>
              </filter>
              <countRange>1~5</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>
        <li>
          <weight>100</weight>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li MayRequire="xmb.ancienturbanruins.mo">AM_MRE</li>
                  <li>MealSurvivalPack</li>
                </thingDefs>
              </filter>
              <countRange>10~40</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>
      </options>
    </root>
  </ThingSetMakerDef>

