  <ThingSetMakerDef>
    <defName>DMSAC_PackLoots</defName>
    <root Class="ThingSetMaker_RandomOption">
      <options>
        <li>
          <weight>10</weight>
          <thingSetMaker Class="ThingSetMaker_MarketValue">
            <fixedParams>
              <filter>
                <categories>
                  <li>Manufactured</li>
                  <li>Medicine</li>
                  <li MayRequire="xmb.ancienturbanruins.mo">AM_Manufactured</li>
                  <li MayRequire="xmb.ancienturbanruins.mo">AM_Electronics</li>
                  <li MayRequire="xmb.ancienturbanruins.mo">AM_Tool</li>
                  <li MayRequire="xmb.ancienturbanruins.mo">AM_SpecialComp</li>
                </categories>
              </filter>
              <countRange></countRange>
              <totalMarketValueRange>1000~2000</totalMarketValueRange>
            </fixedParams>
          </thingSetMaker>
        </li>
        <li>
          <weight>10</weight>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>MealSurvivalPack</li>
                </thingDefs>
              </filter>
              <countRange>40</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>
      </options>
    </root>
  </ThingSetMakerDef>

<!-- 聯動邊雷的引擎戰利品 -->
  <ThingSetMakerDef>
    <defName>DMSAC_RTC_CompLoots</defName>
    <root Class="ThingSetMaker_RandomOption">
      <options>
        <li MayRequire="rimthunder.core">
          <weight>10</weight>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>EngineTurbine</li>
                  <li>EngineJet</li>
                  <li>EngineHeavy</li>
                </thingDefs>
              </filter>
              <countRange>1~1</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>
        <li MayRequire="rimthunder.core">
          <weight>10</weight>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>EngineMedium</li>
                  <li>EngineLight</li>
                </thingDefs>
              </filter>
              <countRange>1~3</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>
        <li>
          <weight>10</weight>
          <thingSetMaker Class="ThingSetMaker_StackCount">
            <fixedParams>
              <filter>
                <thingDefs>
                  <li>ComponentIndustrial</li>
                  <li>ComponentStamped</li>
                </thingDefs>
              </filter>
              <countRange>1~8</countRange>
            </fixedParams>
          </thingSetMaker>
        </li>
      </options>
    </root>
  </ThingSetMakerDef>
</Defs>