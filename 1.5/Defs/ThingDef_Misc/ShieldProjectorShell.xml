<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="BaseBullet">
    <defName>DMSAC_Bullet_9M133Missile</defName>
    <label>anti tank missile</label>
    <graphicData>
      <texPath>Things/Projectiles/DMS_9M133Missile/projectile</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>TransparentPostLight</shaderType>
      <drawSize>1.25</drawSize>
    </graphicData>
    <thingClass>AncientCorps.Projectile_Parabola</thingClass>
    <projectile>
      <damageDef>Bomb</damageDef>
      <speed>40</speed>
      <armorPenetrationBase>1.8</armorPenetrationBase>
      <explosionRadius>1.5</explosionRadius>
      <damageAmountBase>120</damageAmountBase>
      <flyOverhead>true</flyOverhead>
      <explosionRadiusDisplayPadding>3</explosionRadiusDisplayPadding>
      <arcHeightFactor>1</arcHeightFactor>
      <soundHitThickRoof>Artillery_HitThickRoof</soundHitThickRoof>
      <soundExplode>MortarBomb_Explode</soundExplode>
      <soundImpactAnticipate>MortarRound_PreImpact</soundImpactAnticipate>
      <soundAmbient>MortarRound_Ambient</soundAmbient>
    </projectile>
    <comps>
      <li Class="AncientCorps.CompProperties_AfterBurner">
        <lifeTime>60</lifeTime>
        <ExhaustFleck>DMS_Fleck_RocketFlame</ExhaustFleck>
        <ExhaustCurve>
          <points>
            <li>(0,3)</li>
            <li>(0.15,2)</li>
            <li>(0.5,1)</li>
          </points>
        </ExhaustCurve>
        <SmokeFleck>DMS_Fleck_RocketSmokeTrail</SmokeFleck>
        <SmokeCurve>
          <points>
            <li>(0,3)</li>
            <li>(0.5,1)</li>
            <li>(0.9,1)</li>
            <li>(1,0.5)</li>
          </points>
        </SmokeCurve>
      </li>
    </comps>
  </ThingDef>

